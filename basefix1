        NPS.Text = P4.Text
    end
)

P5.MouseButton1Down:connect(
    function()
        PlrsF.Visible = false
        NPS.Text = P5.Text
    end
)

P6.MouseButton1Down:connect(
    function()
        PlrsF.Visible = false
        NPS.Text = P6.Text
    end
)

First = "_G.Text = [[\n"
After = "\n]]\nreturn _G.Text"
Space = [[

]]
MaxL = 0
MaxS = 135
local Copy1, Copy2
local X, Y, Z
local XXX, YYY, ZZZ
Convert.MouseButton1Down:connect(function()
        WoodType = {
            "Fir",
            "Birch",
            "Cherry",
            "Walnut",
            "Generic",
            "Oak",
            "Pine",
            "Palm",
            "Koa",
            "Volcano",
            "GreenSwampy",
            "GoldSwampy",
            "GenericSpecial",
            "SnowGlow",
            "Frost",
            "CaveCrawler",
            "LoneCave",
            "Spooky",
            "SpookyNeon",
			"Bamboo",
            "Grey"
        }


        tag = ""


        local WT = {}
        local function Set(Add, Main)
            for i, v in pairs(WoodType) do
                WT[v .. (Add or "")] = Main
            end
        end

        Set("", "")
        Set("P", 0)
        Set("C", 0)
        Set("CC", 0)

        if NPS.Text ~= "No Player Selected" and NPS.Text ~= "No Player" then
            Label.Text = "Copying"
            wait()
            for i, v in pairs(game.Workspace.Properties:GetChildren()) do
                if v.Owner.Value == game.Players[NPS.Text] then
                    X = v.OriginSquare.Position.X
                    Y = v.OriginSquare.Position.Y
                    Z = v.OriginSquare.Position.Z
                end
            end

            ---------------------------------------------------------------------------------------------------------------------- 

            --if find grey then create blueprint wood class stringval

            for i, v in pairs(game.Workspace.PlayerModels:GetChildren()) do
                if v:FindFirstChild("Owner") and v.Owner.Value ~= nil and v.Owner.Value == game.Players[NPS.Text] then
                    if v:FindFirstChild("BuildDependentWood") then
                        if v:FindFirstChild("MainCFrame") then
                            if tostring(v.BuildDependentWood.BrickColor) == "Medium stone grey" then
                                stringy = Instance.new("StringValue")
                                stringy.Name = "BlueprintWoodClass"
                                stringy.Value = "Grey"
                                stringy.Parent = v
                            end
                        end
                    end
                end
            end
----------------------------------------------------------------------------------------------------------------------print(Plr)
            for i, v in pairs(game:GetService("Workspace").PlayerModels:GetChildren()) do
                if v:FindFirstChild("Owner") and v.Owner.Value == game.Players[NPS.Text] and v:FindFirstChild("BlueprintWoodClass") then
                    local Cframe = (v:FindFirstChild("MainCFrame") and v.MainCFrame.Value) or (v:FindFirstChild("Main") and v.Main.CFrame)
                    x, y, z = Cframe:ToEulerAnglesXYZ()
                    if Cframe.X > X then
                        XXX = (Vector3.new(Cframe.X) - Vector3.new(X)).magnitude
                        XXX = -XXX
                    else
                        XXX = (Vector3.new(Cframe.X) - Vector3.new(X)).magnitude
                    end
                    if Cframe.Y > Y then
                        YYY = (Vector3.new(Cframe.Y) - Vector3.new(Y)).magnitude
                        YYY = -YYY
                    end
                    if Cframe.Z > Z then
                        ZZZ = (Vector3.new(Cframe.Z) - Vector3.new(Z)).magnitude
                        ZZZ = -ZZZ
                    else
                        ZZZ = (Vector3.new(Cframe.Z) - Vector3.new(Z)).magnitude
                    end

                    WT[v.BlueprintWoodClass.Value .. "C"] = WT[v.BlueprintWoodClass.Value .. "C"] + 1
                    WT[v.BlueprintWoodClass.Value .. "CC"] = WT[v.BlueprintWoodClass.Value .. "CC"] + 1
                    WT[v.BlueprintWoodClass.Value .. "P"] = 1
                    WT[v.BlueprintWoodClass.Value] = WT[v.BlueprintWoodClass.Value] .. "Sbp(" .. "'" .. v.ItemName.Value .. "',Tag*Ang(" .. x .. ", " .. y .. ", " .. z .. ") + Vec(" .. -XXX .. ", " .. -YYY .. ", " .. -ZZZ .. "), Plr)" .. Space
                end
            end

            ss = '"'

            for i, v in pairs(WoodType) do
                if WT[v .. "C"] == 0 then
                    WT[v .. "C"] = ""
                else
                    WT[v .. "C"] = "wait(" .. WT[v .. "C"] / Number + Number2 .. ")"
                end

                if WT[v .. "P"] ~= 0 then
                    WT[v .. "P"] = "SetBp(" .. ss .. v .. ss .. ", " .. WT[v .. "CC"] .. ")"
                else
                    WT[v .. "P"] = ""
                end
            end


            local Copy = setclipboard or toclipboard or (syn and syn.write_clipboard)

            local NewMain = First
            
            for i, v in pairs(WoodType) do
                NewMain = NewMain .. WT[v] .. WT[v .. "C"] .. WT[v .. "P"]
            end 
            
            Check  = NewMain  .. tag
            
            NewMain = NewMain .. tag .. After
            
            if Check == "" then
                Label.Text = "No Blueprint To Copy"
                wait(4)
                Label.Text = "By Hacker#8326"
            else
                if Copy then
                    Copy(NewMain)
                    Label.Text = "Copied on Keyboard !"
                    wait(4)
                    Label.Text = "By Hacker#8326"
                end
            end
        else
            Label.Text = "Select One Player!"
            wait(3)
            Label.Text = "By Hacker#8326"
        end
end)
