buttons = {P1, P2, P3, P4, P5, P6}

for i, v in pairs(game:GetService("Players"):GetPlayers()) do
    buttons[i].Text = v.Name
end

game.Players.PlayerAdded:connect(
    function()
        for i, v in pairs(game.Players:GetChildren()) do
            if v == nil then
                buttons[i].Text = "No Player"
            else
                buttons[i].Text = v.Name
            end
        end
    end
)
game.Players.PlayerRemoving:connect(
    function()
        for i, v in pairs(game.Players:GetChildren()) do
            if v == nil then
                buttons[i].Text = "No Player"
            else
                buttons[i].Text = v.Name
            end
        end
    end
)

Close.MouseButton1Down:connect(
    function()
        Main.Visible = false
    end
)

NPS.MouseButton1Down:connect(
    function()
        PlrsF.Visible = true
    end
)

P1.MouseButton1Down:connect(
    function()
        PlrsF.Visible = false
        NPS.Text = P1.Text
    end
)

P2.MouseButton1Down:connect(
    function()
        PlrsF.Visible = false
        NPS.Text = P2.Text
    end
)

P3.MouseButton1Down:connect(
    function()
        PlrsF.Visible = false
        NPS.Text = P3.Text
